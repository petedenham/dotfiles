{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "background": "transparent",
          "foreground": "p:foreground",
          "properties": {
            "style": "full"
          },
          "style": "plain",
          "template": "{{ .Path }}",
          "type": "path"
        },
        {
          "background": "transparent",
          "foreground": "p:grey",
          "properties": {
            "branch_icon": "",
            "commit_icon": "@",
            "fetch_status": true
          },
          "style": "plain",
          "template": " {{ .HEAD }}{{ if or (.Working.Changed) (.Staging.Changed) }}*{{ end }} <p:cyan>{{ if gt .Behind 0 }}⇣{{ end }}{{ if gt .Ahead 0 }}⇡{{ end }}</>",
          "type": "git"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "type": "executiontime",
          "style": "plain",

          "foreground": "p:yellow",
          "background": "transparent",
          "template": " {{ .FormattedMs }}",
          "properties": {
            "threshold": 5000
          }
        }
      ],
      "overflow": "hidden",
      "type": "rprompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "foreground": "p:foreground",
          "background": "transparent",
          "template": "❯",
          "foreground_templates": [
            "{{if gt .Code 0}}p:red{{end}}",
            "{{if eq .Code 0}}p:magenta{{end}}"
          ]
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "palette": {
    "cyan": "#8be9fd",
    "foreground": "#F8F8F2",
    "grey": "#6272a4",
    "red": "#ff5555",
    "yellow": "#f1fa8c",
    "magenta": "#ff79c6"
  },
  "secondary_prompt": {
    "background": "transparent",
    "foreground": "p:magenta",
    "template": "❯❯ "
  },
  "transient_prompt": {
    "background": "transparent",
    "foreground": "#f8f8f2",
    "template": "❯ ",
    "foreground_templates": [
      "{{if gt .Code 0}}p:red{{end}}",
      "{{if eq .Code 0}}p:magenta{{end}}"
    ]
  },
  "version": 3
}
